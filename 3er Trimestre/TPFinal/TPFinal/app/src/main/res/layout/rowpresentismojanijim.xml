        android:id="@+id/tvApellidoJanijPresentismo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/tvDNIJanijPresentismo" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
